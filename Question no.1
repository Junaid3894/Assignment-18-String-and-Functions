/*Write a function to calculate length of the string*/
#include<stdio.h>
int length(char *str);
int main()
{
char str[20];
printf("Enter the string :");
fgets(str,20,stdin);
printf("This string contains %d elements ",length(&str[0]));
return 0;
}
int length(char *str){
   int length=0;
   for(int i=0;str[i];i++){
      length++;
   }
   return length-1;
}
